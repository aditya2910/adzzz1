            <xsd:enumeration value="DTA"></xsd:enumeration>
            <xsd:enumeration value="DTF"></xsd:enumeration>
            <xsd:enumeration value="DTD"></xsd:enumeration>
            <xsd:enumeration value="DTS"></xsd:enumeration>
